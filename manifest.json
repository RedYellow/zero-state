{
  "name": "LAND O LINKS",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "New Tab page for lazies with too many links",
  "homepage_url": "https://github.com/nmai/land-o-links",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "storage"
  ]
}